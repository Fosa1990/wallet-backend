    "express": "4.17.1",
    "express-rate-limit": "^6.4.0",
    "generate-password": "^1.7.0",
    "gravatar": "^1.8.2",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "jimp": "^0.16.1",
    "joi": "^17.6.0",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^8.5.1",
